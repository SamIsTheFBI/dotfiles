#!/bin/sh

get_charge() {
	cat "/sys/class/power_supply/BAT1/capacity"
}


icon=("" "" "" "" "")
charge=$(cat "/sys/class/power_supply/BAT1/capacity")
bat_stat=$(cat "/sys/class/power_supply/BAT1/status")

[ "$bat_stat" == "Charging" ] && ci="" || ci=""

echo "$ci"\ \ $charge%
#echo "$bat_stat"
